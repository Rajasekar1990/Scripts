        <JSR223Sampler guiclass="TestBeanGUI" testclass="JSR223Sampler" testname="S04_T02_RT_JSR223 Sampler" enabled="true">
          <stringProp name="scriptLanguage">groovy</stringProp>
          <stringProp name="parameters"></stringProp>
          <stringProp name="filename"></stringProp>
          <stringProp name="cacheKey">true</stringProp>
          <stringProp name="script">//Capture the total response time of current iteration of S04_T01_Sample1
Long T02 = prev.getParent().getTime();
//Output the response time strored in T01 varaiable to JMeter Log
log.info(String.valueOf(T02));
//Store the T01 response time in JMeter Paramter
vars.put(&quot;T02_RT_Param&quot;, String.valueOf(T02));</stringProp>
